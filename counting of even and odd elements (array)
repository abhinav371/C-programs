#include<stdio.h>
int main()
{
	int n,a[10],i,countodd,counteven;
	printf("enter size : ");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("enter the elements : ");
		scanf("%d",&a[i]);
		if(a[i]%2==0)
		counteven++;
		else
		countodd++;
	}
	printf("even:%d",counteven);
	printf("odd:%d",countodd);
}
