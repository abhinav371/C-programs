#include<stdio.h>
void main()
{
	int a[10][10],m,n,i,j,s=0;
	printf("enter number of rows and colomns in a :");
	scanf("%d%d",&m,&n);
	printf("enter elements of a : ");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	if(m==n)
	{
		for(i=0;i<m;i++)
	    {
		    for(j=0;j<n;j++)
		    {
		    	if(i==j)
		    	{
		    		s=s+a[i][j];
				}
		    }
	    }
	}
	else
	printf("not possible");
	printf("sum of diagonal elements : %d",s);
}
