#include<stdio.h>
void main()
{
	int a[10][10],b[10][10],m,n,i,j;
	printf("enter number of rows and colomns in a :");
	scanf("%d%d",&m,&n);
	printf("enter elements of a : ");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
			b[j][i]=a[i][j];
		}
	}
	printf("matrix is : \n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	}
	printf("transpose of the matrix is : \n");
	for(i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
		    printf("%d\t",b[i][j]);
	    }
	    printf("\n");
	}
}
