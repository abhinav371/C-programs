#include <stdio.h>
int main() 
{
    int n,a,r,res=0;
    printf("Enter a number: ");
    scanf("%d",&n);
    a=n;
    while(a!=0) 
	{
    r=a%10;   
    res+=r*r*r;
    a/=10;
    }
    if (res==n)
    printf("%d is an Armstrong number.",n);
    else
    printf("%d is not an Armstrong number.",n);
    return 0;
}
