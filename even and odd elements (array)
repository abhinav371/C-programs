#include<stdio.h>
void main()
{
	int a[100],n,i,e=0,o=0;
	printf("enter size : ");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	    scanf("%d",&a[i]);
	}
	printf("even numbers : \n");
	for(i=0;i<n;i++)
	{
	    if(a[i]%2==0)
	    printf("%d\n",a[i]);
	}
	printf("odd numbers : \n");
	for(i=0;i<n;i++)
	{
	    if(a[i]%2 != 0)
	    printf("%d\n",a[i]);
	}
}
