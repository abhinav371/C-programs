#include<stdio.h>
void main()
{
	int a[10][10],s=0,m,n,i,j;
	printf("enter number of rows and colomns in a :");
	scanf("%d%d",&m,&n);
	printf("enter elements of a : ");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
            s=s+a[i][j];
		}
	}
	printf("sum is : %d",s);
}
	
