#include <stdio.h>
void Find_Factors(int);   
int main()
{
  int n;    
  printf("Enter number : ");
  scanf("%d",&n);
  printf("Factors of a Number are:\n");
  Find_Factors(n); 
  return 0;
}
void Find_Factors(int n)
{ 
  int i; 
  for (i =1;i<=n;i++)
   {
    if(n%i==0)
     {
       printf("%d",i);
     } 
   }
}
