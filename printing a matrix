#include<stdio.h>
int main()
{
    int i,j,m,n,sum=0,a[10][20];
    printf("Enter number of rows : ");
    scanf("%d",&m);
    printf("Enter number of columns : ");
    scanf("%d",&n); 
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
            sum=sum +a[i][j];
        }
    }
    printf("\nSum of elements = %d", sum);
}
