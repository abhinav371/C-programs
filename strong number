#include<stdio.h>
int fact(int n)
{
int a,b=1;
for (a=1;a<=n;a++)
{
b=b*a;
}
return b;
}
int main ()
{
int n,c,r,rem,sum=0,tmp;
printf("Enter a Number:");
scanf("%d",&n);
tmp = n;
for (tmp=n;n>0;n=n/10)
{
c=1,r=1;
rem=n%10;
r=fact(rem);
sum=sum+r;
}
if (sum==tmp)
{
printf ("%d is a Strong Integer\n\n",tmp);
}
else
{
printf ("%d is Not a Strong Integer\n\n",tmp);
}
return 0;
}
