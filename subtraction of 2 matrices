#include<stdio.h>
void main()
{
	int a[10][10],b[10][10],m,n,r,c,s[10][10],i,j;
	printf("enter number of rows and colomns in a :");
	scanf("%d%d",&m,&n);
	printf("enter number of rows and colomns in b :");
	scanf("%d%d",&r,&c);
	printf("enter elements of a : ");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("enter elements of b : ");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&b[i][j]);
		}
	}
	if(m==r && n==c)
	{
		for(i=0;i<m;i++)
		{
			for(j=0;j<n;j++)
			{
			    s[i][j]=a[i][j]-b[i][j];
		        printf("%d\t",s[i][j]);
			}
			printf("\n");
		}
	}
	else
	{
		printf("addition not possible ");
	}
}
